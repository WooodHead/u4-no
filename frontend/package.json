{
  "name": "u4-frontend",
  "version": "1.0.0",
  "description": "Frontend for u4.no",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "dev": "npm run sass-dev && concurrently --kill-others \"nodemon -w server.js -w routes.js server.js\" \"npm run sass-watch\"",
    "build": "next build",
    "start": "node server.js",
    "sass-watch": "node-sass --source-map true -w ./style/ -o ./static/css ./static/css --use autoprefixer -d ./static/css",
    "sass-dev": "node-sass --source-map true ./style/ -o ./static/css ./static/css --use autoprefixer -d ./static/css",
    "sass-build": "node-sass ./style/ -o ./static/css ./static/css --use autoprefixer -d ./static/css --output-style compressed",
    "heroku-postbuild": "next build && npm run sass-build"
  },
  "engines": {
    "node": "8.4.0"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@sanity/block-content-to-react": "^0.1.4",
    "@sanity/client": "^0.108.0",
    "concurrently": "^3.5.0",
    "eslint": "^4.5.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-compat": "^1.0.4",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jest": "^20.0.3",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.3.0",
    "express": "^4.15.4",
    "get-random-values": "^1.2.0",
    "inuitcss": "^6.0.0-beta.5",
    "littlefoot": "^1.0.0",
    "lodash": "^4.17.4",
    "next": "^3.0.6",
    "next-routes": "^1.0.40",
    "node-sass": "^4.5.3",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-headroom": "^2.1.6",
    "react-scrollable-anchor": "^0.5.0",
    "react-scrollspy": "^3.3.3",
    "react-waypoint": "^7.1.0",
    "sass-mq": "^3.3.2",
    "slugify": "^1.2.1"
  },
  "devDependencies": {
    "nodemon": "^1.12.0",
    "prettier-eslint": "^6.4.2"
  }
}
